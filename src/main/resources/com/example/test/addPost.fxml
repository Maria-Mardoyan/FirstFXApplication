<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="addPost" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.test.AddPostController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <TextArea fx:id="postTextArea" maxWidth="360.0" minWidth="360.0" prefHeight="110.0" prefWidth="360.0">
         <VBox.margin>
            <Insets />
         </VBox.margin></TextArea>
      <Button mnemonicParsing="false" onAction="#onAddPostClick" text="Add Post" />
   </children>

</VBox>
